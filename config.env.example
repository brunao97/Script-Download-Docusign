# Configurações do DocuSign
# Obtenha essas informações no DocuSign Developer Portal
DOCUSIGN_INTEGRATION_KEY=your_integration_key_here
DOCUSIGN_USER_ID=your_user_id_here
DOCUSIGN_ACCOUNT_ID=your_account_id_here

# URLs dos ambientes DocuSign
# Para DEMO/SANDBOX: https://demo.docusign.net/restapi
# Para PRODUÇÃO: https://na1.docusign.net/restapi (ou seu server específico)
DOCUSIGN_BASE_PATH=https://demo.docusign.net/restapi

# Caminho para sua chave privada RSA (gerada com npm run generate-key)
DOCUSIGN_RSA_PRIVATE_KEY_PATH=./private.key

# Configurações de download
DOWNLOAD_FOLDER=./downloads
MAX_CONCURRENT_DOWNLOADS=5
LANGUAGE=pt_BR

# Configurações de rate limiting
RATE_LIMIT_REQUESTS_PER_MINUTE=300
RATE_LIMIT_DELAY_MS=200 